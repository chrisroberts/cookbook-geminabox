[Service]
Type=notify
<% if @tmpdir -%>
Environment="TMPDIR=<%= @tmpdir %>"
<% end -%>
ExecStart=<%= Docker::Helpers.executable(node) %> -d<%= @daemon_options %>

[Install]
Also=geminabox.socket